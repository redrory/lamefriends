

<% if current_user %>
	<div id = "welcome">
		Welcome <%= current_user.first_name %>
		<%= image_tag(@my_pic) %>
		<%= link_to "Sign out", signout_path %>
	</div>

	<div id = "sneaky">
	    <%@rush_me_friends.each do |f| %>

	    	<%= image_tag(@graph.get_picture(f["id"])) %>
	    <% end %>
	</div>

	<% @rush_me_friends.each do |gen| %>
		<% @mutual = @graph.get_object(gen["id"])%>

		<% if @mutual["gender"] == "male" %>
			<% @malesID = @mutual["id"] %>
				<%#=@malesName = @mutual["name"] %>
			M<%= image_tag(@graph.get_picture(@malesID)) %> 
		<% end %>


		

		<% @mf = @mutual["gender"] %>
		<% @mmf= @mf["male"] %>
		<% @fmf= @mf["female"] %>

	
	<% end %>

	



<% else %>
	<%= link_to "Sign in with Facebook", "/auth/facebook" %>

<% end %>